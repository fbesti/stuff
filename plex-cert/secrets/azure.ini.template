# Azure DNS credentials for Certbot
# Copy this file to azure.ini and fill in your actual Azure credentials

dns_azure_sp_client_id = YOUR_AZURE_CLIENT_ID
dns_azure_sp_client_secret = YOUR_AZURE_CLIENT_SECRET
dns_azure_tenant_id = YOUR_AZURE_TENANT_ID
dns_azure_subscription_id = YOUR_AZURE_SUBSCRIPTION_ID

# Optional: specify resource group name if different from domain
# dns_azure_resource_group = YOUR_RESOURCE_GROUP_NAME

# Zone name (usually your domain)
dns_azure_zone1 = bigpapa.work